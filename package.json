  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "host": "node -p \"require('ip').address()\"",
    "date": "node -p \"new Date().toISOString().split('T').splice(0,1).join('')\"",
    "start": "ng serve --open",
    "start:host": "ng serve --host=$(npm run host --silent) --open",
    "build": "ng build",
    "build:prod": "NODE_ENV=production ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "update": "ng update",
    "format": "npx prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.1",
